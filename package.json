{
    "name": "electronreactmobxtemplate",
    "description": "electronreactmobxtemplate build dependencies",
    "version": "1.0.0",
    "main": "index.html",
    "scripts": {
        "start": "ts-node script/start.ts",
        "package": "ts-node script/package.ts",
        "build:dev": "yarn compile:dev && cross-env NODE_ENV=development ts-node script/build.ts",
        "build:prod": "yarn compile:prod && cross-env NODE_ENV=production ts-node script/build.ts",
        "compile:dev": "cross-env NODE_ENV=development parallel-webpack --config app/webpack.development.js",
        "compile:prod": "cross-env NODE_ENV=production parallel-webpack --config app/webpack.production.js"
    },
    "devDependencies": {
        "@types/electron-packager": "^10.1.0",
        "@types/electron-winstaller": "^2.6.0",
        "@types/fs-extra": "^5.0.0",
        "@types/node": "^9.4.6",
        "@types/react": "^16.0.38",
        "@types/react-dom": "^16.0.4",
        "electron": "^1.8.2",
        "electron-packager": "^11.0.1",
        "electron-winstaller": "^2.5.2"
    },
    "dependencies": {
        "awesome-typescript-loader": "^3.4.1",
        "clean-webpack-plugin": "^0.1.18",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.9",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "fs-extra": "^5.0.0",
        "html-webpack-plugin": "^2.30.1",
        "node-sass": "^4.7.2",
        "parallel-webpack": "^2.2.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.1",
        "typescript": "^2.7.1",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^2.0.5",
        "webpack-hot-middleware": "^2.21.0",
        "webpack-merge": "^4.1.1"
    }
}
